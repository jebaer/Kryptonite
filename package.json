{
  "name": "databoks-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "css-build": "node-sass --omit-source-map-url public/sass/styles.sass public/css/styles.css",
    "css-watch": "npm run css-build && npm run css-build -- -w",
    "start": "npm run css-build && npm run serve",
    "beforeCommit": "eslint -c .commit.eslintrc ./src",
    "test": "jest --coverage=false"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "vue": "^2.6.1",
    "vue-axios": "^2.1.5",
    "vue-cookies": "^1.7.2",
    "vue-router": "^3.3.4",
    "vue-toasted": "^1.1.28",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "^4.5.8",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jsdoc": "^29.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.1",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11"
  }
}
