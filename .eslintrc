{
    "parser": "vue-eslint-parser",
    "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2020,
    "sourceType": "module"
    },
    "extends": [
        "plugin:vue/recommended",
        "eslint:recommended",
        "prettier/vue",
        "plugin:prettier/recommended",
        "plugin:cypress/recommended"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "node": true
    },
    "plugins": [
        "prettier",
        "cypress"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ]
    }
}